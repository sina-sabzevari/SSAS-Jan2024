          },
          {
            "name": "Sales Amount",
            "expression": "CALCULATE(SUMX(Sales,Sales[Quantity]*Sales[Unit Price]),TREATAS(VALUES(DimDateParallel[date_mapping]),Sales[Order Date]))"
          },
          {
            "name": "Sales Amount Compare",
            "expression": "CALCULATE(SUMX(Sales,Sales[Quantity]*Sales[Unit Price]),TREATAS(VALUES(DimDateParallelCompare[date_mapping]),Sales[Order Date]))"
          },
          {
            "name": "Sales Amount MTD",
            "expression": [
              "",
              "VAR _current_date=MAX(DimDateParallel[date])",
              "VAR _first_day_month=CALCULATE(MAX(DimDateParallel[date_mapping]),DimDateParallel[type]=\"The first day of month\")",
              "VAR _result=CALCULATE(SUMX(Sales,Sales[Quantity]*Sales[Unit Price]),TREATAS(VALUES(DimDateParallel[date_mapping]),Sales[Order Date]),DATESBETWEEN('Date'[Date],_first_day_month,_current_date))",
              "Return _result"
            ]
          },
          {
            "name": "Measure",
            "expression": [
              " ",
              "SWITCH(TRUE(),",
              "ISINSCOPE('Date'[Month Number]),",
              "Var _current_month=MAX('Date'[Month Number])",
              "VAR _current_year=MAX('Date'[Year])",
              "Return",
              "CALCULATE([Sales Amount],'Date'[Month Number]<=_current_month",
              ",'Date'[Year]=_current_year),",
              "ISINSCOPE('Date'[Year]),",
              "VAR _current_year=MAX('Date'[Year])",
              "Return",
              "CALCULATE([Sales Amount],'Date'[Year]<=_current_year),\"salam\")",
              ""
            ]
          },
          {
            "name": "Sales Amount Dynamic",
            "expression": [
              "",
              "SWITCH(SELECTEDVALUE('CalculatedTable 1'[FilterName],\"Brand\"),",
              "\"Color\",CALCULATE([Sales Amount],TREATAS(VALUES('CalculatedTable 1'[ColumnName]),'Product'[Color])),",
